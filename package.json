{
  "name": "solid-webext-starter",
  "version": "0.0.0",
  "description": "Vite+SolidJS Webextension",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "watch": "vite build --watch",
    "serve:firefox": "web-ext run --start-url \"about:debugging#/runtime/this-firefox\" --source-dir ./dist/",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://github.com\" --source-dir ./dist/",
    "tsc": "tsc",
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint .",
    "webext:lint": "web-ext lint -s ./dist"
  },
  "license": "MIT",
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/fs-extra": "^9.0.13",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webextension-polyfill": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "c8": "^7.11.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-solid": "^0.7.0",
    "fs-extra": "^10.1.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.7.1",
    "solid-testing-library": "^0.3.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vite-plugin-solid": "^2.2.6",
    "vitest": "^0.12.7",
    "web-ext": "^7.1.0",
    "webextension-polyfill": "^0.9.0"
  },
  "dependencies": {
    "solid-js": "^1.4.2"
  }
}
